      <section class="container text-center my-5" style="background-color: black;">
        <h2 class="mb-4" style="font-size: 1.75rem">Join Us</h2>
        <div class="row justify-content-center">
          <div class="col-md-5">
            <div class="card mb-4">
              <img src="/images/login.png" class="card-img-top" alt="Login">
              <div class="card-body">
                <h5 class="card-title">Login</h5>
                <p class="card-text">Access your account to explore more features.</p>
                <a href="/auth/login" class="btn btn-primary">Login</a>
              </div>
            </div>
          </div>
          <div class="col-md-5">
            <div class="card mb-4">
              <img src="/images/register.png" class="card-img-top" alt="Register">
              <div class="card-body">
                <h5 class="card-title">Register</h5>
                <p class="card-text">Create an account to get started.</p>
                <a href="/auth/register" class="btn btn-primary">Register</a>
              </div>
            </div>
          </div>
        </div>
      </section>
      



